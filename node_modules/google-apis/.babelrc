{
  "presets": [
    [
      "env", {
        "targets": {
          "node": "current"
        }
      }
    ],
    "react",
    "stage-0"
  ],
  "env": {
    "test": {
      "plugins": [ "istanbul" ],
      "sourceMaps": true,
      "compact": false
    }
  },
  "sourceMaps": true,
  "compact": false,
  "plugins": [
    [
      "flow-runtime", {
        "assert": true,
        "annotate": true
      }
    ],
    "transform-decorators-legacy",
    [
      "transform-runtime", {
        "helpers": false,
        "polyfill": true,
        "regenerator": false
      }
    ]
  ]
}
